{
    "name": "dcastanera/laravel-clients",
    "description": "Clients package for Laravel 5 that includes contacts and configuration items.",
    "type": "library",
    "authors": [
        {
            "name": "Dan Castanera",
            "email": "dcastanera@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "dcastanera/laravel-notes": "*"
    },
    "autoload": {
        "classmap": [
            "src"
        ],
        "psr-4": {
            "DCastanera\\Clients\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DCastanera\\Clients\\ClientServiceProvider"
            ]
        }
    }
}
